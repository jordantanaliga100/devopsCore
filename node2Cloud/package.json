{
  "name": "node2cloud",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix ",
    "format": "prettier . --write",
    "build": "tsc",
    "start:prod": "NODE_ENV=production  node dist/src/index.js ",
    "start:dev": "NODE_ENV=development tsx watch src/index.ts ",
    "db:generate": "NODE_ENV=development tsx node_modules/.bin/drizzle-kit generate",
    "db:migrate": "NODE_ENV=development tsx node_modules/.bin/drizzle-kit migrate",
    "db:studio": "NODE_ENV=development tsx node_modules/.bin/drizzle-kit studio",
    "docker:dev": "sh ./scripts/dev.sh",
    "docker:prod": "sh ./scripts/prod.sh",
    "git": "sh ./scripts/push.sh",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dotenv-flow": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "winston": "^3.18.3",
    "ws": "^8.18.3",
    "xss-clean": "^0.1.4",
    "xss-clean-types": "^1.0.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
